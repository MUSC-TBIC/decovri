<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
        <name>UmlsTerm</name>
        <version>1</version>
        <vendor>Biomedical Informatics Center at MUSC</vendor>
        <types>
                <typeDescription>
                        <name>org.apache.uima.conceptMapper.UmlsTerm</name>
                        <description>Annotation for dictionary lookup matches</description>
                        <supertypeName>uima.tcas.Annotation</supertypeName>
                        <features>
                                <featureDescription>
                                        <name>PreferredTerm</name>
                                        <description>canonical form</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>ConceptCode</name>
                                        <description>UMLS Metathesaurus Concept Code for the matched term</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>ConceptType</name>
                                        <description>Source reference
					for concept code (e.g., CUI,
					SNOMEDCT_US, etc.)</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>TUI</name>
                                        <description>UMLS Metathesaurus TUI for the matched term</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>BasicLevelConceptCode</name>
                                        <description>UMLS Metathesaurus Concept Code for the basic level term the matched terms fits within</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>BasicLevelConceptType</name>
                                        <description>Source reference
					for the basic level concept
					code (e.g., CUI,
					SNOMEDCT_US, etc.)</description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>enclosingSpan</name>
                                        <description>span that this NoTerm is contained within (i.e. its sentence)</description>
                                        <rangeTypeName>uima.tcas.Annotation</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>matchedText</name>
                                        <description></description>
                                        <rangeTypeName>uima.cas.String</rangeTypeName>
                                </featureDescription>
                                <featureDescription>
                                        <name>matchedTokens</name>
                                        <description></description>
                                        <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                                </featureDescription>
                        </features>
                </typeDescription>
        </types>
</typeSystemDescription>
