<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>NoteSection</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <types>	
    <typeDescription>
      <name>edu.musc.tbic.uima.NoteSection</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>SectionNumber</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>SectionDepth</name>
          <description>Given an hierarchical section schema, how deep is the current section ( 0 = root level/major category)</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>SectionId</name>
          <description>Type (or concept id) of current section</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginHeader</name>
          <description>The start offset for this section's header (-1 if no header)</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endHeader</name>
          <description>The end offset for this section's header (-1 if no header)</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>modifiers</name>
          <description>Modifiers (key/value pairs) associated with the given section</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
